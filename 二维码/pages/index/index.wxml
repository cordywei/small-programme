<!--index.wxml-->
<view class="container">
  <view class="img-box">
    <canvas style="width:686rpx;height:686rpx;background:#f1f1f1;" canvas-id='mycanvas'></canvas>
  </view>
  <form>
    <view class="input-row">
      <label>網址</label>
      <input name="url" type="text" maxlength="255" placeholder='{{placeholder}}'></input>
    </view>
    <button form-type='submit' class="myBtn" type='primary'>生成二維碼</button>
  </form>
</view>